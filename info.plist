				<key>applescript</key>
				<string>on alfred_script(q)
  tell application "Mailplane 3"
      set emailLink to "[url=" &amp; currentURL &amp; "]" &amp; currentTitle &amp; "[/url]"
      set emailSubject to currentTitle
      tell application "Things"
        set newToDo to make new to do with properties {name:emailSubject, notes:emailLink}
      end tell
      activate
      tell application "System Events"
        keystroke "e"
        keystroke "i"
      end tell
    end tell
end alfred_script</string>
